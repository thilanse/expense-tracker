{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "django_gunicorn",
      "image": "thilanse96/expense-tracker-gunicorn",
      "hostname": "django_gunicorn",
      "essential": true,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "thilanse96/expense-tracker-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "django_gunicorn"
      ],
      "memory": 128
    }
  ]
}